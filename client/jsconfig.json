{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "$components": [
                "./src/components/*"
            ],
            "$utils": [
                "./src/utils/*"
            ],
            "$store/types": [
                "./src/store/types"
            ],
            "$store": [
                "./src/store/*"
            ],
            "$pages": [
                "./src/pages/*"
            ],
            "$src": [
                "./src/*"
            ]
        }
    }
}